{
  "name": "rizzui",
  "version": "0.8.7",
  "license": "MIT",
  "description": "A Modern and Minimal React UI Library built with TailwindCSS. Designed to provide you with a simple and intuitive set of UI components that are easy to use, customize and integrate into your React application.",
  "homepage": "https://rizzui.com/",
  "author": {
    "name": "REDQ",
    "url": "https://redq.io/"
  },
  "bugs": {
    "url": "https://github.com/rizzui/rizzui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rizzui/rizzui.git"
  },
  "keywords": [
    "RizzUI",
    "rizzui",
    "rizz ui",
    "components",
    "React Components",
    "react ui"
  ],
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist coverage",
    "lint": "tsc",
    "dev": "concurrently \"pnpm build --watch\"",
    "build": "tsup",
    "test": "jest --coverage",
    "test:ci": "pnpm test --ci",
    "test:watch": "jest --watch"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "clsx": "2.1.1",
    "react-number-format": "5.4.2",
    "tailwind-merge": "2.6.0"
  },
  "peerDependencies": {
    "@floating-ui/react": ">=0.25.4",
    "@headlessui/react": ">=2.0.3",
    "react": ">=19",
    "react-dom": ">=19"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@headlessui/react": "2.2.0",
    "@tailwindcss/forms": "0.5.9",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/react": "^19.0.2",
    "autoprefixer": "10.4.20",
    "babel-loader": "9.2.1",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "9.1.0",
    "css-loader": "7.1.2",
    "fs-extra": "11.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.49",
    "postcss-loader": "8.1.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "react": "19.0.0",
    "react-docgen-typescript-plugin": "1.0.8",
    "style-loader": "4.0.0",
    "tailwindcss": "3.4.17",
    "ts-jest": "29.2.5",
    "tsconfig": "workspace:*",
    "tsconfig-paths-webpack-plugin": "4.2.0",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
